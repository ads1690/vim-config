set tabstop=3
set nowrap
set number
set smartindent
set smarttab
set expandtab
set autoindent
set paste

set shiftwidth=3 
colorscheme koehler 
set background=dark
set incsearch
set hlsearch
syntax on
nmap <F8> :TagbarToggle<CR>
nmap <F7> :NERDTree<CR>

filetype plugin indent on
set ofu=syntaxcomplete#Complete

